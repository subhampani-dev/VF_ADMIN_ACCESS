{
  "name": "VF_ADMIN_ACCESS",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.26.4",
    "@sap-cloud-sdk/resilience": "^3.26.4",
    "@sap/cds": "^8",
    "@sap/hana-client": "^2.24.21",
    "@sap/xsenv": "^5.5.0",
    "@sap/xssec": "^4.5.0",
    "axios": "^1.8.4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.8.2",
    "@sap/hdi-deploy": "4.9.5"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "[production]": {
      "requires": {
        "db": "hana",
        "auth": "xsuaa"
      }
    },
    "[development]": {
      "requires": {
        "auth": {
          "kind": "mocked",
          "users": {
            "admin@user.com": {}
          }
        },
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": ":memory:"
          }
        }
      }
    },
    "[hybrid]": {
      "requires": {
        "db": "hana",
        "auth": "dummy"
      }
    },
    "requires": {
      "destinations": true,
      "account-service-api": {
        "kind": "rest",
        "credentials": {
          "destination": "account-service-api"
        }
      },
      "provisioning-service-api": {
        "kind": "rest",
        "credentials": {
          "destination": "provisioning-service-api"
        }
      },
      "service-manager-api": {
        "kind": "rest",
        "credentials": {
          "destination": "service-manager-api"
        }
      },
      "BTP_Management_API": {
        "kind": "rest",
        "credentials": {
          "destination": "BTP_Management_API"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}

{
  "name": "VF_ADMIN_ACCESS",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xsenv": "^5.5.0",
    "@sap/xssec": "^4.5.0",
    "axios": "^1.8.4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.8.2"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "[production]": {
      "requires": {
        "db": "hana",
        "auth": "xsuaa"
      }
    },
    "[development]": {
      "requires": {
        "auth": {
          "kind": "mocked",
          "users": {
            "admin@user.com": {}
          }
        },
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": ":memory:"
          }
        }
      }
    },
    "[hybrid]": {
      "requires": {
        "db": "hana",
        "auth": "xsuaa"
      }
    },
    "requires": {
      "Accounts.Service": {
        "kind": "rest",
        "model": "srv/external/APIAccountsService"
      },
      "destinations": true
    }
  }
}
